{
	"camper type": {
		"allegiance": {
			"type:": "radio",
			"options": ["junior", "senior", "leader"],
			"validation": {
				"required": true
			}
		}
	},
	"discounts": {
		"chocolate": {
			"type": "select",
			"data": "{{chocolate_data}}"
		},
		"early bird": {
			"type": "radio",
			"data": "{{early_bird_data}}",
			"options": ["yes", "no"],
			"validation": {
				"required": true
			}
		},

		"married" : {
			"text": "are you married?",
			"type": "radio",
			"options": ["yes", "no"],
			"conditions": {
				"allegiance": "leader"
			},
			"validation": {
				"required": true
			}
		}
	},
	"basic details": {
		"name": {
			"type": "text",
			"validation": {
				"required": true,
				"max_length": 255,
				"is_text": true
			}
		},
		"camper email": {
			"type": "email"
		},
		"birth date": {
			"type": "date",
			"validation": {
				"required": true
			}
		},
		"year in school": {
			"type": "select",
			"options": [6, 7, 8, 9, 10, 11, 12],
			"conditions": {
				"allegiance": "camper"
			},
			"validation": {
				"required": true
			}
		},
		"school": {
			"type": "text",
			"conditions": {
				"allegiance": "camper"
			},
			"validation": {
				"required": true,
				"is_text": true,
				"max_length": 255
			}
		},
		"contact number": {
			"type": "phone_number",
			"validation": {
				"required": true
			}
		},
		"address": {
			"type": "address",
			"validation": {
				"required": true
			}
		}
	},
	"medical contact information" : {
		"medicare number": {
			"type": "text",
			"validation": {
				"required": true,
				"min_length": 11,
				"max_length": 11
			}
		},
		"medicare card reference number": {
			"type": "number",
			"validation": {
				"required": true,
				"min_length": 1,
				"max_length": 1
			}
		},
		"medicare card expiry date": {
			"type": "date_month_year",
			"validation": {
				"required": true
			}
		},
		"private health insurance company": {
			"type": "text",
			"validation": {
				"max_length": 255
			}
		},
		"private health insurance membership number": {
			"type": "text",
			"validation": {
				"max_length": 255
			}
		},
		"family doctor": {
			"type": "text",
			"validation": {
				"required": true,
				"max_length": 255
			}
		},
		"doctor contact number": {
			"type": "phone_number",
			"validation": {
				"required": true
			}
		}
	},
	"medical information": {
		"date of last tetanus booster": {
			"type": "date",
			"info": "if you are unsure - enter 01/01/1900",
			"validation": {
				"required": true
			}
		}
	}
}